<!DOCTYPE html>
<html>
<head>
  <title>Dashboard Fullscreen</title>
  <%= csrf_meta_tags %>
  <%= csp_meta_tag %>

  <%= stylesheet_link_tag "https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css", media: "all" %>
  <%= stylesheet_link_tag "https://cdn.jsdelivr.net/npm/bootstrap-icons@1.11.3/font/bootstrap-icons.css" %>
  <%= stylesheet_link_tag "application", media: "all" %>

  <%= javascript_include_tag "https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js", defer: true %>
  <%= javascript_include_tag "https://cdn.jsdelivr.net/npm/chartkick@5.1.0/dist/chartkick.min.js" %>
  <%= javascript_include_tag "https://cdn.jsdelivr.net/npm/chart.js@4.4.1/dist/chart.umd.min.js" %>
  <%= javascript_include_tag "application", defer: true %>
</head>
<body class="bg-light">
  <div class="container-fluid p-4">
    <%= yield %>
  </div>
</body>
</html>